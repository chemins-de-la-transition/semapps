{
  "name": "cdlt-middleware",
  "private": true,
  "description": "Middleware for Les Chemins de la Transition",
  "keywords": [
    "semapps",
    "semantic"
  ],
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner --instances=max services",
    "link-semapps-packages": "yarn link @semapps/activitypub @semapps/auth @semapps/backup @semapps/fuseki-admin @semapps/importer @semapps/inference @semapps/jsonld @semapps/ldp @semapps/middlewares @semapps/mime-types @semapps/push @semapps/signature @semapps/sparql-endpoint @semapps/triplestore @semapps/webacl @semapps/webfinger @semapps/webhooks @semapps/webid",
    "unlink-semapps-packages": "yarn unlink @semapps/activitypub @semapps/auth @semapps/backup @semapps/fuseki-admin @semapps/importer @semapps/inference @semapps/jsonld @semapps/ldp @semapps/middlewares @semapps/mime-types @semapps/pod @semapps/push @semapps/signature @semapps/sparql-endpoint @semapps/triplestore @semapps/webacl @semapps/webfinger @semapps/webhooks @semapps/webid"
  },
  "dependencies": {
    "@semapps/auth": "0.3.24",
    "@semapps/backup": "0.3.24",
    "@semapps/fuseki-admin": "0.3.24",
    "@semapps/inference": "0.3.24",
    "@semapps/jsonld": "0.3.24",
    "@semapps/ldp": "0.3.24",
    "@semapps/mime-types": "0.3.24",
    "@semapps/sparql-endpoint": "0.3.24",
    "@semapps/triplestore": "0.3.24",
    "@semapps/webacl": "0.3.24",
    "@semapps/webid": "0.3.24",
    "dotenv-flow": "^3.2.0",
    "handlebars": "^4.7.7",
    "ioredis": "^4.17.3",
    "moleculer": "^0.14.18",
    "moleculer-cron": "^0.0.2",
    "moleculer-mail": "^1.2.5",
    "moleculer-repl": "^0.6.3",
    "moleculer-web": "^0.10.0-beta1",
    "multiparty": "^4.2.2",
    "speakingurl": "^14.0.1",
    "url-join": "^4.0.1"
  }
}
