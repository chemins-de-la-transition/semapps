{
  "name": "cdlt-middleware",
  "private": true,
  "description": "Middleware for Les Chemins de la Transition",
  "keywords": [
    "semapps",
    "semantic"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development moleculer-runner --repl --hot services importers/*.js",
    "start": "cross-env NODE_ENV=production moleculer-runner --instances=max services importers/*.js",
    "link-semapps-packages": "yarn link @semapps/activitypub @semapps/auth @semapps/backup @semapps/importer @semapps/inference @semapps/jsonld @semapps/ldp @semapps/middlewares @semapps/migration @semapps/mime-types @semapps/mirror @semapps/pod @semapps/signature @semapps/sparql-endpoint @semapps/triplestore @semapps/void @semapps/webacl @semapps/webfinger @semapps/webhooks @semapps/webid",
    "unlink-semapps-packages": "yarn unlink @semapps/activitypub @semapps/auth @semapps/backup @semapps/importer @semapps/inference @semapps/jsonld @semapps/ldp @semapps/middlewares @semapps/migration @semapps/mime-types @semapps/mirror @semapps/pod @semapps/signature @semapps/sparql-endpoint @semapps/triplestore @semapps/void @semapps/webacl @semapps/webfinger @semapps/webhooks @semapps/webid"
  },
  "dependencies": {
    "@rdfjs/data-model": "^1.3.4",
    "@semapps/activitypub": "0.4.0-rc.0",
    "@semapps/auth": "0.4.0-rc.0",
    "@semapps/backup": "0.4.0-rc.0",
    "@semapps/importer": "0.4.0-rc.0",
    "@semapps/inference": "0.4.0-rc.0",
    "@semapps/jsonld": "0.4.0-rc.0",
    "@semapps/ldp": "0.4.0-rc.0",
    "@semapps/migration": "0.4.0-rc.0",
    "@semapps/mime-types": "0.4.0-rc.0",
    "@semapps/mirror": "0.4.0-rc.0",
    "@semapps/signature": "0.4.0-rc.0",
    "@semapps/sparql-endpoint": "0.4.0-rc.0",
    "@semapps/triplestore": "0.4.0-rc.0",
    "@semapps/void": "0.4.0-rc.0",
    "@semapps/webacl": "0.4.0-rc.0",
    "@semapps/webfinger": "0.4.0-rc.0",
    "@semapps/webid": "0.4.0-rc.0",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "handlebars": "^4.7.7",
    "node-fetch": "^2.6.7",
    "ioredis": "^4.17.3",
    "moleculer": "^0.14.18",
    "moleculer-bull": "0.2.8",
    "moleculer-cron": "^0.0.2",
    "moleculer-mail": "^1.2.5",
    "moleculer-repl": "^0.6.3",
    "moleculer-web": "^0.10.0-beta1",
    "multiparty": "^4.2.2",
    "sharp": "^0.31.2",
    "speakingurl": "^14.0.1",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  }
}
